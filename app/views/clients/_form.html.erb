
<%= panel_for title: 'Add client', size: 12 do %>
  <div class="panel-body">

   <div class="row">
   <div class="col-sm-12">

    <%= form_for @client do |f| %>

        <div class="form-group">
          <%= f.text_field :name, placeholder: "Client name",  class: "form-control" %>
        </div>

        <%= f.submit class: "btn btn-lg btn-primary btn-block" %>

    <% end %>

  </div>
</div>

  </div>

  <div class="panel-footer">

  </div>
<% end %>





